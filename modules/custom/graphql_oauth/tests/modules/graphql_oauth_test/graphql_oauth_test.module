<?php

/**
 * @file
 * Contains graphql_oauth_test.module.
 */

/**
 * Implements hook_graphql_schema_extension_alter().
 */
function graphql_oauth_test_graphql_schema_extension_alter(array &$info): void {
  $plugin_id = 'graphql_oauth_schema_extension';
  if (array_key_exists($plugin_id, $info)) {
    $info[$plugin_id]['schema'] = 'graphql_oauth_test';
  }
}
