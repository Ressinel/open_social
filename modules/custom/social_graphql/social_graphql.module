<?php

/**
 * @file
 * Contains social_graphql.module.
 */

/**
 * Implements hook_graphql_schema_extension_alter().
 */
function social_graphql_graphql_schema_extension_alter(array &$info): void {
  $plugin_id = 'social_graphql_oauth2_schema_extension';
  if (array_key_exists($plugin_id, $info)) {
    $info[$plugin_id]['schema'] = 'open_social';
  }
}
